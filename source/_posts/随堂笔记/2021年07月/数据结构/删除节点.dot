digraph demo8{
  rankdir=LR;
  node [shape=record];
  head [shape=ellipse];
  p [shape=ellipse];
  c [shape=ellipse];
  n1 [label="{5|n1}"]
  n2 [label="{10|n2}"]
  n3 [label="{20|null}"]
  n4 [label="{30|null}"]


  head -> n1
  n1 -> n2
  n2 -> n3
  // n3 -> n4
  c->n4
  p ->n3
}




